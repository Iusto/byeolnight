<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Spring Boot 관련 거짓 양성 억제 -->
    <suppress>
        <notes>Spring Boot 내부 의존성 관련 거짓 양성</notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- QueryDSL 관련 거짓 양성 억제 -->
    <suppress>
        <notes>QueryDSL 코드 생성 관련 거짓 양성</notes>
        <packageUrl regex="true">^pkg:maven/com\.querydsl.*$</packageUrl>
        <cve>CVE-2022-42003</cve>
    </suppress>
    
    <!-- 테스트 의존성 관련 억제 -->
    <suppress>
        <notes>테스트 환경에서만 사용되는 의존성</notes>
        <packageUrl regex="true">^pkg:maven/.*test.*$</packageUrl>
        <cve>CVE-2023-20863</cve>
    </suppress>
</suppressions>